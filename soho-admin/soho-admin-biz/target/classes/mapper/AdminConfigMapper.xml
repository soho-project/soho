<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="work.soho.admin.mapper.AdminConfigMapper">
   <sql id="AllField">
       id,
       key,
       value,
       type,
       group_name
   </sql>
   <select id="getById" resultType="work.soho.api.admin.po.AdminConfig">
       SELECT 
           <include refid="AllField"></include>
       FROM admin_config: table where id=#{id}
   </select>
   <insert id="insert" parameterType="work.soho.api.admin.po.AdminConfig">
       INSERT INTO admin_config: table
       <trim prefix="(" suffix=")" suffixOverrides=","> 
      <if test="id != null">
           id,
       </if>
      <if test="key != null">
           key,
       </if>
      <if test="value != null">
           value,
       </if>
      <if test="type != null">
           type,
       </if>
      <if test="groupName != null">
           group_name,
       </if>
       </trim>
       <trim prefix="VALUES(" suffix=")" suffixOverrides=","> 
      <if test="id != null">
           id,
       </if>
      <if test="key != null">
           key,
       </if>
      <if test="value != null">
           value,
       </if>
      <if test="type != null">
           type,
       </if>
      <if test="groupName != null">
           groupName,
       </if>
       </trim>
   </insert>
  <update id="update"  parameterType="work.soho.api.admin.po.AdminConfig">
      <set>
          <if test="id != null">id=#{id}</if>
          <if test="key != null">key=#{key}</if>
          <if test="value != null">value=#{value}</if>
          <if test="type != null">type=#{type}</if>
          <if test="groupName != null">group_name=#{groupName}</if>
      </set>
  </update>


</mapper>
